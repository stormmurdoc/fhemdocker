# Template .gplot file for creating a new SVG instance

set terminal png transparent size <SIZE> crop
set output '<OUT>.png'
set xdata time
set timefmt "%Y-%m-%d_%H:%M:%S"
set xlabel " "

set title '<TL>'
set ylabel "Humidity"
set y2label "Temperature"
set grid
set ytics
set y2tics

